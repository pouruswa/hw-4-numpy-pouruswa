   "source": [
    "import csv\n",
    "import numpy as np\n",
    "\n",
    "#filename is a text string containing the name and extension of a file containing student scores. \n",
    "#For example, `grades_example.csv`.  See the example file for details about the file structure.\n",
    "\n",
    "#numExams is an integer.  It tells us how many exams are in the gradebook.  \n",
    "#Exams will always be in the last column(s).\n",
    "\n",
    "#hwWeight is a float in [0,1].  \n",
    "#This value tells us the percentage of the overall grade that is composed of the student's average homework percentage.  \n",
    "#The remaining percentage will be applied to the average exam percentage.  \n",
    "#For example, if hwWeight = 0.3, then the exams are weighted by 0.7 (0.3 + 0.7 = 1.0).\n",
    "\n",
    "def gradeInfo(filename, numExams, hwWeight):\n",
    "    totalmarks = column1 = None\n",
    "    data = []\n",
    "    grades = []\n",
    "    floatformat = \"{:.2f}\".format\n",
    "    np.set_printoptions(formatter = {'float_kind':floatformat})\n",
    "    with open(filename, 'r') as csvfile:\n",
    "        csvreader = csv.reader(csvfile, delimiter = ',')\n",
    "        for i, j in enumerate(csvreader):\n",
    "            if i<2:\n",
    "                pass\n",
    "            elif i==2:\n",
    "                totalmarks = j\n",
    "            elif i==3:\n",
    "                column1 = j\n",
    "                column1.remove('')\n",
    "                totalmarks = totalmarks[:len(column1)]\n",
    "                totalmarks.remove('')\n",
    "                totalmarks = [float(x) for x in totalmarks]\n",
    "                homeworktotal = np.asarray(totalmarks[:-numExams])\n",
    "                numhomework = len(homeworktotal)\n",
    "                examtotal = np.asarray(totalmarks[-numExams:])\n",
    "                scaledmarks = [1.0] + [100/x for x in totalmarks]\n",
    "            else :\n",
    "                if j[0][0]=='%':\n",
    "                    pass\n",
    "                else :\n",
    "                    j = j[:len(column1)]\n",
    "                    j = [0 if x==''  else float(x) for x in j]\n",
    "                    homework =  j[1:len(column1)-numExams]\n",
    "                    exam = j[len(homework)+1:]\n",
    "                    data.append(j)\n",
    "                    grade = round(((sum(homework/homeworktotal)/numhomework)*hwWeight + sum(exam/examtotal)/numExams*(1-hwWeight))*100, 1)\n",
    "                    grades.append([j[0], grade])\n",
    "                    \n",
    "    Array1 = np.asarray(data, dtype = np.float64)\n",
    "    Array1 = Array1*scaledmarks\n",
    "    \n",
    "    #(a)\n",
    "    mean = np.mean(Array1, axis = 0)[1]\n",
    "    \n",
    "    #(b)\n",
    "    Array2 = Array1[:, [0, 2]]\n",
    "    Array2 = Array2[Array2[:, 1].argsort()][::-1]\n",
    "    \n",
    "    #(c)\n",
    "    Array3 = Array1[(Array1[:, 1] >= 90) & (Array1[:, 3] >= 90)]\n",
    "    Array3 = Array3[:, 0]\n",
    "    \n",
    "    #(d)\n",
    "    Array4 = Array1[(Array1[:, 1] <= 80) & (Array1[:, 2] >= 90)]\n",
    "    Array4 = len(Array4)\n",
    "    \n",
    "    #(e)\n",
    "    Array5 = np.asarray(grades,dtype = np.float64)\n",
    "    \n",
    "    return mean, Array2, Array3, Array4, Array5\n",
    "    \n",
    "gradeInfo('grades_example.csv', 1, 0.5)"
   ]
